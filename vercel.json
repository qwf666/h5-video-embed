{
  "version": 2,
  "buildCommand": "pnpm -w run vercel-build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "functions": {
    "api/video.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    },
    "api/video/parse.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    },
    "api/proxy/parse.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    },
    "api/proxy/bilibili/parse.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy/bilibili/parse",
      "destination": "/api/proxy/bilibili/parse.js"
    },
    {
      "source": "/api/proxy/parse",
      "destination": "/api/proxy/parse.js"
    },
    {
      "source": "/api/video/parse",
      "destination": "/api/video/parse.js"
    },
    {
      "source": "/api/video",
      "destination": "/api/video.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
